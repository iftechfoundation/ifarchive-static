<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/">
   <ShortName>IF Archive</ShortName>
   <Description>Search IF Archive</Description>
   <InputEncoding>UTF-8</InputEncoding>
   <Image width="16" height="16" type="image/x-icon">https://ifarchive.org/misc/icon-16.ico</Image>
   <Image width="64" height="64" type="image/png">https://ifarchive.org/misc/icon-64.png</Image>
   <Url type="text/html" method="POST" template="https://search.ifarchive.org/search">
      <Param name="searchstr" value="{searchTerms}"/>
      <Param name="pagenum" value="{startPage}"/>
   </Url>
   <SearchForm>https://search.ifarchive.org/search</SearchForm>
</OpenSearchDescription>
